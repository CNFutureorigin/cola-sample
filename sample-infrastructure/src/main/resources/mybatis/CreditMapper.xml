<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.futureorigin.architecture.sample.cola.gatewayimpl.database.mapper.CreditMapper">

    <select id="findByUserId" resultType="net.futureorigin.architecture.sample.cola.gatewayimpl.database.dataobject.credit.CreditDO">
